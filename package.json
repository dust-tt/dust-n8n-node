{
	"name": "n8n-nodes-dust",
	"version": "0.1.4",
	"description": "Dust for n8n: Talk to your AI agents and upload documents to Dust",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://dust.tt",
	"author": {
		"name": "Alban Dumouilla - Dust",
		"email": "alban@dust.tt"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/dust-tt/dust-n8n-node.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@10.12.4",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/DustApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Dust/Dust.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^8.36.0",
		"eslint": "^9.30.1",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.1",
		"n8n-workflow": "^1.82.0",
		"prettier": "^3.6.2",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"publishConfig": {
		"access": "public"
	}
}
